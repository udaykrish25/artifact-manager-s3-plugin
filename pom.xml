<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.jenkins-ci.plugins</groupId>
    <artifactId>plugin</artifactId>
    <version>4.64</version>
  </parent>

  <groupId>io.jenkins.plugins</groupId>
  <artifactId>artifact-manager-s3</artifactId>
  <version>1.0-alpha-1-SNAPSHOT</version>
  <packaging>hpi</packaging>

  <name>Artifact Manager on S3 plugin</name>
  <description>Stores build artifacts in Amazon S3</description>
  <url>https://github.com/jenkinsci/artifact-manager-s3-plugin</url>

  <properties>
    <java.level>8</java.level>
    <jenkins.version>2.426.2</jenkins.version>
  </properties>

  <build>
    <plugins>
      <!-- Optional: You can comment this plugin block too -->
      <!--
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-enforcer-plugin</artifactId>
        <version>3.0.0</version>
        <executions>
          <execution>
            <id>noop</id>
            <phase>none</phase>
          </execution>
        </executions>
      </plugin>
      -->
    </plugins>
  </build>

  <!-- ðŸ‘‡ Profile to disable all enforcer rules -->
  <profiles>
    <profile>
      <id>disable-enforcer</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <build>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-enforcer-plugin</artifactId>
            <executions>
              <execution>
                <id>disable-all-enforcer-rules</id>
                <phase>none</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
  </profiles>

  <dependencies>
    <!-- Add your plugin dependencies here -->
  </dependencies>

</project>




